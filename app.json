{
  "expo": {
    "name": "ResetDopa",
    "slug": "resetdopa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "description": "Your companion for dopamine detox and habit building. Track urges, complete daily tasks, and build lasting streaks.",
    "primaryColor": "#4A90E2",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "url": "https://u.expo.dev/resetdopa"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dopareset.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "com.dopareset.app",
      "versionCode": 8,
      "targetSdkVersion": 35,
      "compileSdkVersion": 35,
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4A90E2"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "SCHEDULE_EXACT_ALARM",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#4A90E2",
          "sounds": []
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 21,
            "buildToolsVersion": "35.0.0",
            "kotlinVersion": "1.9.10",
            "gradleVersion": "8.10",
            "gradleProperties": [
              "android.useAndroidX=true",
              "android.enableJetifier=true",
              "org.gradle.jvmargs=-Xmx4096m",
              "org.gradle.parallel=true",
              "android.compileSdkVersion=35",
              "android.targetSdkVersion=35"
            ]
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ],
      "expo-font",
      "expo-asset",
      "expo-web-browser"
    ],
    "extra": {
      "privacyPolicyUrl": "https://resetdopa.com/privacy.html",
      "termsUrl": "https://resetdopa.com/terms.html",
      "eas": {
        "projectId": "2607ba58-f3f0-45c7-a15c-8a92e06721c3"
      }
    },
    "runtimeVersion": "1.0.0",
    "scheme": "com.dopareset.app",
    "owner": "p18khr12"
  }
}

{
  "expo": {
    "name": "ResetDopa",
    "slug": "resetdopa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "description": "Your companion for dopamine detox and habit building. Track urges, complete daily tasks, and build lasting streaks.",
    "primaryColor": "#4A90E2",
    "jsEngine": "jsc",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "url": "https://u.expo.dev/resetdopa"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dopareset.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "com.dopareset.app",
      "versionCode": 6,
      "jsEngine": "jsc",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4A90E2"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "SCHEDULE_EXACT_ALARM",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#4A90E2",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "privacyPolicyUrl": "https://p18khr.github.io/dopareset-legal/privacy.md",
      "termsUrl": "https://p18khr.github.io/dopareset-legal/terms.md",
      "eas": {
        "projectId": "21b8164e-fe5c-442f-b614-983f60df5e81"
      }
    },
    "runtimeVersion": "1.0.0",
    "scheme": "com.dopareset.app",
    "owner": "p18khr1234"
  }
}
